	local x_flip = VS2:FindParam("xflip", t)
	local y_pct = VS2:FindParam("y", t)
	local y_flip = VS2:FindParam("yflip", t)
	local play = VS2:FindParam("repeat", t)
	local layer = VS2:FindParam("layer", t)
	local fadetime = VS2:FindParam("fadetime", t)
	local actor = VS2:FindParam("actor", t)
	local emote = VS2:FindParam("emote", t)
	local from = VS2:FindParam("from", t)
	local base = VS2:FindParam("base", t)
	local scale = VS2:FindParam("scale", t)
	local alpha = VS2:FindParam("alpha", t)
	local scalewithbg = VS2:FindParam("scalewithbg", t)
	
