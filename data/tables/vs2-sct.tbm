$State: GS_STATE_GAME_PLAY
$On Gameplay Start:
[
	--if VS2.Enabled then
		VS2:Exit(false, true)
	--end
]

$On State Start:
[
	if VS2.Running and VS2.ShowCursor then
		io.setCursorHidden(false)
	end	
]

$On State End:
[
	if VS2.Running then
		VS2.StayInGame = false
		VS2:Exit()
		AXUI.Ingame = false
		AXUI:ScreenInit()
	end
]

$On Message Received:
[

	if VS2.Enabled and AXMessage.DrawControl == "VS2" then
		AXMessage:SetupMessage(hv.SenderString, hv.Name, hv.Message)
	end
]

$On Frame:
[
	if VS2.Enabled then
		if not VS2.SDL2 then
			io.setCursorHidden(false)
		end
		VS2:CheckTimers(ba.getFrametimeOverall())
		VS2:Draw()
		
		if (not VS2.Lockdown) and mn.getMissionTime() < 0.1 then
			if Lockdown then
				Lockdown()
				VS2.Lockdown = true
			end
		end
		
		--if VS2.Debug then
			--PrintDebug(VS2.BGAlt)
		--end

	end
	
	if Scroll and Scroll.Enabled then
		Scroll:Draw()
	end
	
	if VS2.ExpireTalk then
		VS2.ExpireTalk = nil
		VS2.CanTalk = nil
		ba.print("VN: expiring talk status\n")
	end
	
	if VS2.CanTalk then
		VS2.ExpireTalk = true
		ba.print("VN: expire talk next frame\n")
	end
			
]

$On Mouse Released:
[
	if VS2.Enabled then
		VS2:Actions(hv.MouseButton, false)
	end
]

$On Mouse Pressed:
[
	if VS2.Enabled then
		VS2:Actions(hv.MouseButton, true)
	end
]

$On Key Released:
[
	if hv.Key == "Alt-C" then
		VS2.CanTalk = true
	end
		
	if VS2.Enabled then
	
		VS2:CheckInterrupt(hv.Key)
	
		if hv.Key == "0" then
			if VS2.Debug then
				VS2.Debug = false
			else
				VS2.Debug = true
			end
		end
		
		if hv.Key == "-" then
			if VS2.ShowBox then
				VS2.ShowBox = false
			else
				VS2.ShowBox = true
			end
		end
		
		--Break in case of emergency
		--if hv.Key == "=" then
		--	DumpToFile(VS2, "VNS")
		--	DumpToFile(AXMessage, "AXM")
		--end
	end

]

